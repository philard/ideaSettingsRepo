<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Source Code Pro" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_BREADCRUMBS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\bnote\b.*" />
    <filter name="note">
      <pattern index="2" />
    </filter>
    <filter name="todo">
      <pattern index="0" />
    </filter>
  </component>
</application>